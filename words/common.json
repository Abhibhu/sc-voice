{
    "alphabet": "^[a-z'\u02bc\u2019]*$",
    "wordEnd": "([^a-zA-Z].*)?$",
    "ipa": {
        "vowels":"aeiouj",
        "pli": {
            "a": "\u0250",
            "a$": "a\u0308",
            "ah": "\u0250",
            "^a": "\u0250\u02c8",
            "\u0101": "\u0251",
            "aya": "aj\u0250\u02cc",
            "bh": "b\u02b0",
            "\u1e0d": "d",
            "dh": "d\u02b0",
            "du": "d\u028a",
            "\u012b": "i\u02d0",
            "\u1e6d\u1e6dh": "\u0288\u0306\u0288\u02b0",
            "gh": "g\u02b0",
            "unused-h": "h\u0308",
            "i": "\u026a",
            "j": "\u029d",
            "jj": "\u029d.\u029d",
            "kh": "k\u02b0",
            "kk": "k.k",
            "kkh": "k.k\u02b0",
            "ll": "l.l",
            "\u00f1": "\u0272",
            "\u1e37":"\u029f\u0308",
            "\u1e47": "\u014b",
            "ma": "ma",
            "ng": "\u014bg",
            "nu": "nu",
            "\u1e45g": "\u014bg",
            "\u1e45k": "\u014bk",
            "sutta": "\u02ccsutta\u0308",
            "r": "\u027a",
            "tt": "t.t",
            "unused-th": "\u0288\u02b0",
            "unused-u": "\u028A",
            "\u016b": "\u028a\u02d0",
            "^va": "va",
            "y": "j"
        }
    },
    "romanize": {
        "\u0101": "a",
        "\u1e0d": "d",
        "\u1e25": "h",
        "\u012b": "i",
        "\u1e37": "l",
        "\u1e39": "l",
        "\u1e41": "m",
        "\u1e43": "m",
        "\u1e47": "n",
        "\u1e45": "n",
        "\u00f1": "n",
        "\u1e5b": "r",
        "\u1e5d": "r",
        "\u1e63": "s",
        "\u015b": "s",
        "\u1e6d": "t",
        "\u016b": "u"
    },
    "symbols": {
        "\n": {
            "name": "end of line",
            "eol": true
        },
        "!": {
            "name": "exclamation mark",
            "cuddle": "left",
            "endSegment": true
        },
        "\"": {
            "name": "quotation mark"
        },
        "#": {
            "name": "number sign"
        },
        "$": {
            "name": "dollar sign"
        },
        "%": {
            "name": "percent"
        },
        "&": {
            "name": "ampersand"
        },
        "'": {
            "name": "apostrophe"
        },
        "(": {
            "name": "left parenthesis",
            "cuddle": "right"
        },
        ")": {
            "name": "right parenthesis",
            "cuddle": "left"
        },
        "*": {
            "name": "asterisk"
        },
        "+": {
            "name": "plus sign"
        },
        ",": {
            "name": "comma",
            "cuddle": "left"
        },
        "-": {
            "name": "hyphen minus",
            "isWord": true
        },
        "{": {
            "name": "left brace"
        },
        "}": {
            "name": "right brace"
        },
        ".": {
            "name": "full stop",
            "endSegment": true,
            "cuddle": "left"
        },
        "/": {
            "name": "solidus"
        },


        ":": {
            "name": "colon"
        },
        ";": {
            "name": "semicolon"
        },
        "<": {
            "name": "less-than sign"
        },
        "=": {
            "name": "equals sign"
        },
        ">": {
            "name": "greater-than sign"
        },
        "?": {
            "name": "question mark",
            "endSegment": true,
            "cuddle": "left"
        },
        "@": {
            "name": "commercial at"
        },

        "[": {
            "name": "left square bracket",
            "cuddle": "right"
        },
        "]": {
            "name": "right square bracket"
        },

        "\u2014": {
            "name": "em dash",
            "cuddle": "both"
        },
        "\u2018": {
            "name": "left single quote",
            "cuddle": "right"
        },
        "\u2019": {
            "name": "right single quote",
            "cuddle": "left",
            "isWord": true
        },
        "\u02bc": {
            "name": "apostrophe",
            "cuddle": "left",
            "isWord": true
        },
        "\u201C": {
            "name": "left double quote",
            "cuddle": "right"
        },
        "\u201D": { 
            "name": "right double quote",
            "cuddle": "left"
        },
        "\u2026": { 
            "name": "ellipsis",
            "cuddle": "left"
        }
    }
}
