{
    "alphabet": "^[a-z]*$",
    "wordEnd": "([^a-zA-Z].*)?$",
    "romanize": {
        "\u0101": "a",
        "\u1e0d": "d",
        "\u1e25": "h",
        "\u012b": "i",
        "\u1e37": "l",
        "\u1e39": "l",
        "\u1e41": "m",
        "\u1e43": "m",
        "\u1e47": "n",
        "\u1e45": "n",
        "\u00f1": "n",
        "\u1e5b": "r",
        "\u1e5d": "r",
        "\u1e63": "s",
        "\u015b": "s",
        "\u1e6d": "t",
        "\u016b": "u"
    },
    "symbols": {
        "!": {
            "name": "exclamation mark",
            "cuddle": "left",
            "endSegment": true
        },
        "\"": {
            "name": "quotation mark"
        },
        "#": {
            "name": "number sign"
        },
        "$": {
            "name": "dollar sign"
        },
        "%": {
            "name": "percent"
        },
        "&": {
            "name": "ampersand"
        },
        "'": {
            "name": "apostrophe"
        },
        "(": {
            "name": "left parenthesis",
            "cuddle": "right"
        },
        ")": {
            "name": "right parenthesis",
            "cuddle": "left"
        },
        "*": {
            "name": "asterisk"
        },
        "+": {
            "name": "plus sign"
        },
        ",": {
            "name": "comma",
            "cuddle": "left"
        },
        "-": {
            "name": "hyphen minus"
        },
        ".": {
            "name": "full stop",
            "endSegment": true,
            "cuddle": "left"
        },
        "/": {
            "name": "solidus"
        },


        ":": {
            "name": "colon"
        },
        ";": {
            "name": "semicolon"
        },
        "<": {
            "name": "less-than sign"
        },
        "=": {
            "name": "equals sign"
        },
        ">": {
            "name": "greater-than sign"
        },
        "?": {
            "name": "question mark",
            "endSegment": true,
            "cuddle": "left"
        },
        "@": {
            "name": "commercial at"
        },

        "[": {
            "name": "left square bracket",
            "cuddle": "right"
        },
        "]": {
            "name": "right square bracket"
        },

        "\u2018": {
            "name": "left single quote",
            "cuddle": "right"
        },
        "\u2019": {
            "name": "right single quote",
            "cuddle": "left"
        },
        "\u201C": {
            "name": "left double quote",
            "cuddle": "right"
        },
        "\u201D": { 
            "name": "right double quote",
            "cuddle": "left"
        }
    },

    "words": {
        "bhikku": "bhikkhu",
        "bikkhu": "bhikkhu",
        "bhikkhu":{
            "ipa":"b\u026aku\u02D0(.)"
        },
        "bhikkus": "bhikkhus",
        "bikkhus": "bhikkhus",
        "bhikkhus":{
            "ipa":"b\u026aku\u02D0z(.)"
        },
        "earth": {
            "ipa": "\u025c:r\u03b8(.)"
        },
        "supreme": {
            "ipa":"s\u0259\u02c8pri\u02d0m"
        },
        "tathagata": "tath\u0101gata", 
        "tath\u0101gata": {
            "ipa": "t\u0259\u02c8t\u0251\u02d0\u0261\u0259t\u0259(.)"
        },
        "tathagatas": "tath\u0101gatas",
        "tath\u0101gatas": {
            "ipa": "t\u0259\u02c8t\u0251\u02d0\u0261\u0259t\u0259z(.)"
        }
    }
}
